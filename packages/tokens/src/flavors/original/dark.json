{
	"$description": "Dark mode overrides for 'original' flavor. Only includes tokens that differ from light mode (index.json). Automatically applied via @media (prefers-color-scheme: dark) and [flavor-mode='dark'].",
	"color": {
		"background": {
			"base": { "value": "{color.neutral.950.value}", "type": "color" },
			"surface": { "value": "{color.neutral.900.value}", "type": "color" },
			"raised": { "value": "{color.neutral.800.value}", "type": "color" },
			"overlay": { "value": "{color.neutral.950.value}", "type": "color" },
			"emphasis": { "value": "{color.neutral.700.value}", "type": "color" }
		},
		"focus": {
			"ring": { "value": "{color.brand.400.value}", "type": "color" },
			"ring-offset": { "value": "{color.neutral.950.value}", "type": "color" },
			"background": { "value": "{color.brand.950.value}", "type": "color" },
			"text": { "value": "{color.brand.100.value}", "type": "color" }
		},
		"text": {
			"heading": { "value": "{color.neutral.50.value}", "type": "color" },
			"body": { "value": "{color.neutral.200.value}", "type": "color" },
			"caption": { "value": "{color.neutral.400.value}", "type": "color" },
			"link": {
				"default": { "value": "{color.brand.400.value}", "type": "color" },
				"hover": { "value": "{color.brand.300.value}", "type": "color" },
				"active": { "value": "{color.brand.200.value}", "type": "color" },
				"visited": { "value": "{color.brand.500.value}", "type": "color" },
				"focus": { "value": "{color.brand.400.value}", "type": "color" }
			},
			"muted": { "value": "{color.neutral.500.value}", "type": "color" },
			"on-solid": { "value": "{color.neutral.950.value}", "type": "color" }
		},
		"icon": {
			"default": { "value": "{color.neutral.400.value}", "type": "color" },
			"muted": { "value": "{color.neutral.500.value}", "type": "color" },
			"emphasis": { "value": "{color.neutral.100.value}", "type": "color" },
			"interactive": { "value": "{color.brand.400.value}", "type": "color" },
			"on-solid": { "value": "{color.neutral.950.value}", "type": "color" }
		},
		"border": {
			"muted": { "value": "{color.neutral.700.value}", "type": "color" },
			"default": { "value": "{color.neutral.600.value}", "type": "color" },
			"emphasis": { "value": "{color.brand.400.value}", "type": "color" }
		},
		"action": {
			"solid": {
				"background": {
					"default": { "value": "{color.brand.600.value}", "type": "color" },
					"hover": { "value": "{color.brand.500.value}", "type": "color" }
				},
				"text": {
					"default": { "value": "{color.neutral.950.value}", "type": "color" }
				}
			},
			"outline": {
				"border": {
					"default": { "value": "{color.neutral.600.value}", "type": "color" },
					"hover": { "value": "{color.brand.400.value}", "type": "color" }
				},
				"text": {
					"default": { "value": "{color.neutral.200.value}", "type": "color" }
				}
			},
			"ghost": {
				"background": {
					"hover": { "value": "{color.neutral.800.value}", "type": "color" }
				},
				"text": {
					"default": { "value": "{color.neutral.200.value}", "type": "color" }
				}
			},
			"disabled": {
				"background": { "value": "{color.neutral.800.value}", "type": "color" },
				"text": { "value": "{color.neutral.600.value}", "type": "color" },
				"border": { "value": "{color.neutral.700.value}", "type": "color" },
				"opacity": { "value": "{opacity.600.value}", "type": "number" }
			}
		},
		"state": {
			"destructive": {
				"background": {
					"value": "{color.state.error.900.value}",
					"type": "color"
				},
				"text": { "value": "{color.state.error.300.value}", "type": "color" },
				"border": { "value": "{color.state.error.600.value}", "type": "color" }
			},
			"success": {
				"background": {
					"value": "{color.state.success.900.value}",
					"type": "color"
				},
				"text": { "value": "{color.state.success.300.value}", "type": "color" },
				"border": {
					"value": "{color.state.success.600.value}",
					"type": "color"
				}
			}
		},
		"selection": {
			"background": { "value": "{color.brand.800.value}", "type": "color" },
			"text": { "value": "{color.neutral.50.value}", "type": "color" }
		}
	}
}
